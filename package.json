{
  "name": "on-the-hill-theatre-club-website",
  "private": true,
  "scripts": {
    "build": "keystone build && next build",
    "build:keystone": "keystone build",
    "build:next": "next build",
    "deploy": "pnpm build && pnpm migrate",
    "dev": "concurrently \"pnpm dev:keystone\" \"pnpm dev:next\"",
    "dev:keystone": "keystone dev",
    "dev:next": "next dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "postinstall": "keystone postinstall && ts-gql build",
    "migrate": "keystone prisma migrate deploy",
    "start:keystone": "next start .keystone/admin",
    "start:next": "next start",
    "ts-gql:build": "ts-gql build",
    "ts-gql:watch": "ts-gql watch"
  },
  "dependencies": {
    "@apollo/server": "^4.3.0",
    "@as-integrations/next": "^1.2.0",
    "@aws-sdk/signature-v4-crt": "^3.226.0",
    "@headlessui/react": "^1.7.7",
    "@heroicons/react": "^2.0.13",
    "@keystone-6/core": "4.0.1",
    "@keystone-6/fields-document": "^6.0.0",
    "@preconstruct/next": "^4.0.0",
    "@prisma/client": "4.3.1",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.8",
    "@ts-gql/compiler": "^0.16.1",
    "@ts-gql/next": "^17.0.0",
    "@ts-gql/tag": "^0.7.0",
    "autoprefixer": "^10.4.13",
    "aws-crt": "^1.15.5",
    "clsx": "^1.2.1",
    "fast-glob": "^3.2.12",
    "feed": "^4.2.2",
    "focus-visible": "^5.2.0",
    "graphql": "^16.6.0",
    "graphql-request": "^5.1.0",
    "next": "^13.0.7",
    "next-auth": "^4.18.7",
    "postcss-focus-visible": "^6.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remark-gfm": "^3.0.1",
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "@ts-gql/eslint-plugin": "^0.8.4",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "concurrently": "^7.6.0",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "typescript": "^4.9.4"
  },
  "packageManager": "pnpm@7.15.0",
  "ts-gql": {
    "schema": "schema.graphql",
    "mode": "no-transform"
  }
}