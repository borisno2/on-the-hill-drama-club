    "@types/node": "^20.10.0",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "eslint": "8.54.0",
    "eslint-config-next": "14.0.3",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
