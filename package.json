    "@mapbox/rehype-prism": "^0.8.0",
    "@mdx-js/loader": "^2.1.5",
    "@mdx-js/react": "^2.1.5",
    "@next/mdx": "^13.0.2",
    "@preconstruct/next": "^4.0.0",
    "@prisma/client": "4.3.1",
    "@tailwindcss/typography": "^0.5.4",
    "autoprefixer": "^10.4.12",
    "clsx": "^1.2.1",
    "fast-glob": "^3.2.11",
    "feed": "^4.2.2",
    "focus-visible": "^5.2.0",
    "graphql": "^16.6.0",
