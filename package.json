{
  "name": "on-the-hill-theatre-club-website",
  "private": true,
  "scripts": {
    "build": "keystone build && next build",
    "build:keystone": "keystone build",
    "build:next": "next build",
    "deploy": "pnpm build && pnpm migrate",
    "dev": "keystone postinstall --fix && turbo dev:keystone dev:next",
    "dev:next-ssl": "node server.js",
    "dev:keystone": "next dev .keystone/admin --port 4000",
    "dev:keystone:dev": "keystone dev",
    "dev:next": "next dev --port 3000",
    "dev:prep": "keystone postinstall --fix && ts-gql build && turbo build:keystone && keystone prisma db push",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "postinstall": "keystone postinstall && ts-gql build",
    "migrate": "keystone prisma migrate deploy",
    "seed-data": "tsx seed-data.ts",
    "migrate:gen": "keystone postinstall --fix && keystone prisma migrate dev",
    "start:keystone": "next start .keystone/admin",
    "start:next": "next start",
    "ts-gql:build": "ts-gql build",
    "ts-gql:watch": "ts-gql watch"
  },
  "dependencies": {
    "@apollo/server": "^4.3.2",
    "@as-integrations/next": "^1.2.0",
    "@aws-sdk/signature-v4-crt": "^3.264.0",
    "@headlessui/react": "^1.7.8",
    "@heroicons/react": "^2.0.14",
    "@keystone-6/core": "4.0.1",
    "@keystone-6/fields-document": "^6.0.0",
    "@marsidev/react-turnstile": "^0.0.5",
    "@next/font": "^13.1.6",
    "@preconstruct/next": "^4.0.0",
    "@prisma/client": "4.3.1",
    "@sendgrid/mail": "^7.7.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@ts-gql/compiler": "^0.16.3",
    "@ts-gql/next": "^17.0.0",
    "@ts-gql/tag": "^0.7.0",
    "@types/dotenv-flow": "^3.2.0",
    "@vercel/analytics": "^0.1.8",
    "autoprefixer": "^10.4.13",
    "aws-crt": "^1.15.9",
    "clsx": "^1.2.1",
    "csrf": "^3.1.0",
    "cuid": "^2.1.8",
    "dayjs": "^1.11.7",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.0.3",
    "dotenv-flow": "^3.2.0",
    "fast-glob": "^3.2.12",
    "feed": "^4.2.2",
    "focus-visible": "^5.2.0",
    "formik": "^2.2.9",
    "graphql": "^16.6.0",
    "graphql-request": "^5.1.0",
    "inngest": "^0.9.3",
    "intuit-oauth": "^4.0.0",
    "next": "^13.1.6",
    "next-auth": "^4.19.2",
    "node-quickbooks": "^2.0.41",
    "postcss-focus-visible": "^6.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-tailwindcss-datepicker": "^1.4.2",
    "remark-gfm": "^3.0.1",
    "tailwindcss": "^3.2.4",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@ts-gql/eslint-plugin": "^0.8.5",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "eslint": "8.29.0",
    "eslint-config-next": "13.1.6",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "turbo": "^1.7.3",
    "typescript": "^4.9.5"
  },
  "packageManager": "pnpm@7.15.0",
  "ts-gql": {
    "schema": "schema.graphql",
    "mode": "no-transform"
  }
}