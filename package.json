    "build:next": "next build",
    "dev": "concurrently \"pnpm dev:keystone\" \"pnpm dev:next\"",
    "dev:keystone": "keystone dev",
    "dev:next": "next dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
