{
  "name": "on-the-hill-theatre-club-website",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:keystone": "keystone build",
    "deploy": "pnpm build && pnpm migrate",
    "dev": "turbo dev:watch dev:keystone dev:next",
    "dev:next-ssl": "node server.js",
    "dev:keystone": "next dev .keystone/admin --port 4000",
    "dev:next": "next dev --port 3000",
    "dev:watch": "keystone dev --no-server",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "postinstall": "keystone build --no-ui --frozen && ts-gql build",
    "migrate": "keystone prisma migrate deploy",
    "seed-data": "tsx seed-data.ts",
    "migrate:gen": "keystone build --no-ui && keystone prisma migrate dev",
    "ts-gql:build": "ts-gql build",
    "ts-gql:watch": "ts-gql watch"
  },
  "dependencies": {
    "@apollo/server": "^4.9.4",
    "@as-integrations/next": "^2.0.2",
    "@aws-sdk/signature-v4-crt": "^3.418.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.3.1",
    "@keystone-6/core": "^5.7.0",
    "@marsidev/react-turnstile": "^0.3.1",
    "@prisma/client": "5.4.1",
    "@sendgrid/mail": "^7.7.0",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@ts-gql/compiler": "^0.16.3",
    "@ts-gql/next": "^17.0.0",
    "@ts-gql/tag": "^0.7.0",
    "@types/dotenv-flow": "^3.3.1",
    "@vercel/analytics": "^1.1.0",
    "autoprefixer": "^10.4.16",
    "aws-crt": "^1.18.0",
    "clsx": "^2.0.0",
    "csrf": "^3.1.0",
    "cuid": "^3.0.0",
    "dayjs": "^1.11.10",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.3.1",
    "dotenv-flow": "^4.0.0",
    "focus-visible": "^5.2.0",
    "graphql": "^16.8.1",
    "inngest": "^2.7.2",
    "intuit-oauth": "^4.0.0",
    "next": "^13.5.4",
    "next-auth": "^4.23.2",
    "node-quickbooks": "^2.0.43",
    "postcss-focus-visible": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-tailwindcss-datepicker": "^1.6.6",
    "tailwindcss": "^3.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@ts-gql/eslint-plugin": "^0.9.0",
    "@types/lodash": "^4.14.199",
    "@types/node": "^18.18.4",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.11",
    "eslint": "8.50.0",
    "eslint-config-next": "13.5.4",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.5",
    "tsx": "^3.13.0",
    "turbo": "^1.10.15",
    "typescript": "^5.2.2"
  },
  "packageManager": "pnpm@8.8.0",
  "ts-gql": {
    "schema": "schema.graphql",
    "mode": "no-transform"
  }
}
