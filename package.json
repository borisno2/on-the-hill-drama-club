    "autoprefixer": "^10.4.16",
    "aws-crt": "^1.19.0",
    "clsx": "^2.0.0",
    "csrf": "^3.1.0",
    "cuid": "^3.0.0",
    "dayjs": "^1.11.10",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.3.1",
    "dotenv-flow": "^4.0.1",
    "focus-visible": "^5.2.0",
    "graphql": "^16.8.1",
    "inngest": "^3.6.1",
    "intuit-oauth": "^4.0.0",
